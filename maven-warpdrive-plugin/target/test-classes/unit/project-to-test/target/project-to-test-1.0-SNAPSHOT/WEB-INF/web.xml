<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>Testapp</display-name>
  <filter>
    <filter-name>net.kristianandersen.warpdrive.filter.WarpDriveFilter</filter-name>
    <filter-class>net.kristianandersen.warpdrive.filter.WarpDriveFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>net.kristianandersen.warpdrive.filter.WarpDriveFilter</filter-name>
    <url-pattern>/js/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>net.kristianandersen.warpdrive.filter.WarpDriveFilter</filter-name>
    <url-pattern>/css/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>net.kristianandersen.warpdrive.filter.WarpDriveFilter</filter-name>
    <url-pattern>/images/*</url-pattern>
  </filter-mapping>
  <mime-mapping>
    <extension>css.gz</extension>
    <mime-type>text/css</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>js.gz</extension>
    <mime-type>text/javasscript</mime-type>
  </mime-mapping>
</web-app>

